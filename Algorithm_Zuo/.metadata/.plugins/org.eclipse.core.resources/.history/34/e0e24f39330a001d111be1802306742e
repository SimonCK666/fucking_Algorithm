package class01TimeSort;

public class Test {
	public static void process1() {
		int N = 1000;
		int a = 0;
		for (int i = 0; i < N; i++) {
			a= 2 + 5;
			a= 4 * 7;
			a= 6 * 8;
		}
	}
	
	public static void process2() {
		int N = 1000;
		int a = 0;
		for (int i = 0; i < N; i++) {
			a= 2 | 5;
			a= 4 & 7;
			a= 6 ^ 8;
		}
	}
	
//	process1 & process2 的时间复杂度一致，对比常数项的速度，所以只能跑代码来看谁更优
	
	public static void main(String[] args) {
		
	}
}
